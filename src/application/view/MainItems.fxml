<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.FlowPane?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.view.MainItemsController">
   <center>
      <FlowPane hgap="20.0" onDragDetected="#gotoitemspart" prefHeight="200.0" prefWidth="200.0" vgap="20.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="ItemlistId" mnemonicParsing="false" onAction="#gotoitemspart" prefHeight="120.0" prefWidth="120.0" text="ITEMS ">
               <FlowPane.margin>
                  <Insets top="130.0" />
               </FlowPane.margin>
            </Button>
            <Button fx:id="BiddingListId" mnemonicParsing="false" onAction="#gotobiddingpart" onDragDetected="#gotobiddingpart" prefHeight="120.0" prefWidth="120.0" text="BIDDING">
               <FlowPane.margin>
                  <Insets top="130.0" />
               </FlowPane.margin>
            </Button>
            <Button fx:id="SellingListId" mnemonicParsing="false" onAction="#gotosellingpart" prefHeight="120.0" prefWidth="120.0" text="SELLING">
               <FlowPane.margin>
                  <Insets top="130.0" />
               </FlowPane.margin>
            </Button>
            <Button fx:id="ShippingListId" mnemonicParsing="false" prefHeight="120.0" prefWidth="120.0" text="SHIPPING">
               <FlowPane.margin>
                  <Insets top="130.0" />
               </FlowPane.margin>
            </Button>
         </children>
         <padding>
            <Insets left="30.0" top="25.0" />
         </padding>
      </FlowPane>
   </center>
</BorderPane>
